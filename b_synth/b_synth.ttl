@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix lv2atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2ev: <http://lv2plug.in/ns/ext/event#> .
@prefix lv2pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rg: <http://gareus.org/oss/lv2/> .

<http://gareus.org/>
	a foaf:Person ;
	foaf:name "Robin Gareus" ;
	foaf:mbox <mailto:robin@gareus.org> ;
	foaf:homepage <http://gareus.org/> .

rg:bsynth-out a lv2pg:Group ;
   a lv2pg:StereoGroup ;
   lv2:symbol "out" .

<http://gareus.org/oss/lv2/b_synth>
	a lv2:Plugin , lv2:InstrumentPlugin;
	doap:maintainer <http://gareus.org/> ;
	doap:name "setBfree DSP tonewheel Organ";
	doap:license <http://usefulinc.com/doap/licenses/gpl> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:requiredFeature <http://lv2plug.in/ns/ext/uri-map> ;
	lv2:port
	[
		a lv2:InputPort, lv2atom:AtomPort ;
		lv2atom:bufferType lv2atom:Sequence ;
		lv2:index 0 ;
		lv2atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
		lv2:symbol "in";
		lv2:name "MIDI In";
	],
	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "outL" ;
		lv2:name "Left output" ;
    lv2pg:inGroup rg:bsynth-out ;
    lv2pg:role lv2pg:leftChannel ;
	],	
	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "outR" ;
		lv2:name "Right Output" ;
    lv2pg:inGroup rg:bsynth-out ;
    lv2pg:role lv2pg:rightChannel ;
	]
	.
