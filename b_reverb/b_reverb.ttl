@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://gareus.org/>
	a foaf:Person ;
	foaf:name "Robin Gareus" ;
	foaf:mbox <mailto:robin@gareus.org> ;
	rdfs:seeAlso <http://gareus.org/> .

<http://gareus.org/oss/lv2/b_reverb>
	a lv2:Plugin ,
		lv2:B_Reverb ;
	doap:maintainer <http://gareus.org/> ;
	doap:name "B Organ Reverb";
	doap:license <http://opensource.org/licenses/gpl-2.0> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port
	[
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In"
	],	
	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "mix" ;
		lv2:name "Dry/Wet";
		lv2:default 0.3;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	]
	.
